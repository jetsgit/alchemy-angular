<<%= @element_name %>>
<%- @contents.each do |content| -%>
  <%- if content['type'] == 'EssenceRichtext' -%>
  <div ng-bind-html="safe(ingredient('<%= content['name'] %>').value)" class="safe(ingredient('<%= content['name'] %>'))"></div>
  <%- elsif content['type'] == 'EssencePicture' -%>
  <img ng-if="safe(ingredient('<%= content['name'] %>').value) != ''" ng-src="{{safe(ingredient('<%= content['name'] %>').value)}}">
  <%- else -%>
  <div class="<%= content['name'] %>">{{"safe(ingredient('<%= content['name'] %>').value")}}</div>
  <%- end -%>
<%- end -%>
</<%= @element_name %>>
